<div class="center">

<h2>Sign Up</h2>

<% if @user.errors.any? %>
  <div class="error">
  <p>
  <% @user.errors.full_messages.each do |msg| %>
    <%= msg %>.<br>
  <% end %>
  </p>
  </div>
<% end %>

<% if @auth %>
<%= render partial: 'oauth'%>
<% else %>
<%= render partial: 'form'%>
<% end %>

</div>
