<%= form_tag "/auth/identity/callback" do %>
  <div class="field">
  <%= label_tag :auth_key, "Email:" %>
  <%= text_field_tag :auth_key %>
  </div>
  <div class="field">
  <%= label_tag :password, "Password:" %>
  <%= password_field_tag :password %>
  </div>
  <div class="actions"><%= submit_tag "Log in" %></div>
<% end %>
